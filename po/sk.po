#: ../src/ejecter.c:243
msgid ""
"Drive was removed without ejecting\n"
"Please use menu to eject before removal"
msgstr ""
"Zariadenie bolo odstránené bez odpojenia\n"
"Použite, prosím, ponuku na odpojenie pred jeho odstránením"

#: ../src/ejecter.c:271
#, c-format
msgid ""
"%s has been ejected\n"
"It is now safe to remove the device"
msgstr ""
"%s bolo odpojené\n"
"Teraz môžete zariadenie bezpečne odstrániť"

#: ../src/ejecter.c:277
#, c-format
msgid ""
"Failed to eject %s\n"
"%s"
msgstr ""
"Nepodarilo sa odpojiť %s\n"
"%s"

#: ../src/ejecter.c:455
msgid "Select a drive in menu to eject safely"
msgstr "Vyberte zariadenie, ktoré chcete bezpečne odpojiť"

#: ../src/ejecter.c:558 ../src/ejecter.c:568 ../src/ejecter.cpp:41
msgid "Ejecter"
msgstr "Bezpečné odpojenie"

#: ../src/ejecter.c:560 ../src/ejecter.cpp:37
msgid "Hide icon when no devices"
msgstr "Skryť ikonu pri chýbajúcich zariadeniach"

#: ../src/ejecter.c:569
msgid "Ejects mounted drives"
msgstr "Odpojí pripojené zariadenia"

