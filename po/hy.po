#: ../plugins/ejecter/ejecter.c:188
msgid ""
"Drive was removed without ejecting\n"
"Please use menu to eject before removal"
msgstr ""
"Կրիչն անջատվեց՝ առանց ապահով հեռացման\n"
"Անջատելուց առաջ մենյուից ապահով հեռացրեք սարքը"

#: ../plugins/ejecter/ejecter.c:216
msgid ""
"%s has been ejected\n"
"It is now safe to remove the device"
msgstr ""
"%s -ն հեռացված է\n"
"Այժմ անվտանգ է սարքն հեռացնելու համար"

#: ../plugins/ejecter/ejecter.c:222
msgid ""
"Failed to eject %s\n"
"%s"
msgstr ""
"%s հեռացումը ձախողվեց\n"
"%s"

#: ../plugins/ejecter/ejecter.c:471
msgid "Select a drive in menu to eject safely"
msgstr "Մենոյուից ընտրեք կրիչն անվտանգ հեռացնելու համար"

#: ../plugins/ejecter/ejecter.c:530 ../plugins/ejecter/ejecter.c:540
msgid "Ejecter"
msgstr "Արտամղիչ"

#: ../plugins/ejecter/ejecter.c:532
msgid "Hide icon when no devices"
msgstr "Թաքցնել պատկերը, երբ սարքը բացակայում է"

#: ../plugins/ejecter/ejecter.c:541
msgid "Ejects mounted drives"
msgstr "Հեռացնում է տեղակայված կրիչները"
